{
  "name": "e-commerce",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathIgnorePatterns=.*\\.e2e-spec\\.ts$",
    "test:integration": "jest --testPathPattern=.*\\.integration-spec\\.ts$",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:staged": "jest --findRelatedTests",
    "docker:build": "docker build -t zenstore-api .",
    "docker:run": "docker run -p 3000:3000 zenstore-api",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.yml up",
    "docker:stop": "docker-compose down",
    "deploy:prod": "npm run build && npm run docker:build",
    "security:audit": "npm audit && npm run lint",
    "pretest": "npm run lint",
    "pretest:e2e": "npm run build",
    "test:smoke": "jest --testNamePattern='smoke' --verbose",
    "test:performance": "jest --testNamePattern='performance' --verbose --runInBand"
  },
